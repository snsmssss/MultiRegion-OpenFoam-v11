FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0/battery1";
    object      cellToRegion;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    walls
    {
        type            zeroGradient;
    }
    battery1_to_Air
    {
        type            calculated;
        value           uniform 0;
    }
}


