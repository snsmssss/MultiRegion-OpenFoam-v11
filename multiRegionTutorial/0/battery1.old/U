FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volVectorField;
    location    "0/battery1";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0.01 0 0);

boundaryField
{
    walls
    {
        type            calculated;
        value           uniform (0.01 0 0);
    }
    battery1_to_Air
    {
        type            calculated;
        value           uniform (0 0 0);
    }
}


