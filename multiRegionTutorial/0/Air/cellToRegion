FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0/Air";
    object      cellToRegion;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 1;

boundaryField
{
    inlet
    {
        type            zeroGradient;
    }
    outlet
    {
        type            zeroGradient;
    }
    walls
    {
        type            zeroGradient;
    }
    Air_to_battery1
    {
        type            calculated;
        value           uniform 0;
    }
}


