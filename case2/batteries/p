FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/batteries";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 100000;

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    inlet
    {
        type            calculated;
        value           $internalField;
    }
    outlet
    {
        type            calculated;
        value           $internalField;
    }
    

    walls
    {
        type            calculated;
        value           $internalField;
    }

    back
    {
	type		symmetry;
    }
   
   "batteries_to_.*"
    {
        type            calculated;
        value           uniform 0;
    }
}


