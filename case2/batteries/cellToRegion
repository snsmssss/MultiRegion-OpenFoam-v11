FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/batteries";
    object      cellToRegion;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    walls
    {
        type            zeroGradient;
    }
    back
    {
        type            symmetry;
    }
    batteries_to_Air
    {
        type            calculated;
        value           uniform 0;
    }
    batteries_to_silicaGel
    {
        type            calculated;
        value           uniform 0;
    }
}


