FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/topCoolant";
    object      cellToRegion;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 3;

boundaryField
{
    inlet
    {
        type            zeroGradient;
    }
    outlet
    {
        type            zeroGradient;
    }
    walls
    {
        type            zeroGradient;
    }
    back
    {
        type            symmetry;
    }
    topCoolant_to_silicaGel
    {
        type            calculated;
        value           uniform 0;
    }
}


